<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <gradient android:angle="-90" android:startColor="#4D5059" android:endColor="#4D5059"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <solid android:color="#4D5059"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <gradient android:angle="-90" android:startColor="#191C25" android:endColor="#191C25" />
        </shape>
    </item>
</selector>